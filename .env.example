# Application
NODE_ENV=development
PORT=3000
APP_NAME=inversifyjs-fastifyjs-api

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/app_db?schema=public"

# Authentication
JWT_SECRET=your-jwt-secret-key-should-be-changed-in-production
JWT_EXPIRATION=1h

# Logging
LOG_LEVEL=debug
LOG_DIR=logs
SERVICE_NAME=inversifyjs-fastify-app
LOG_MAX_SIZE=20m
LOG_MAX_FILES=14d

# CORS
CORS_ORIGIN=*
